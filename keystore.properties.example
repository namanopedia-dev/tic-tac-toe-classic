# Keystore properties for signing the release APK/AAB
# Copy this file to keystore.properties and fill in your actual values
# DO NOT commit keystore.properties to version control!

storeFile=../release/tictactoe-release-key.jks
keyAlias=tictactoe-key
storePassword=your-store-password
keyPassword=your-key-password

# To generate a keystore, run:
# keytool -genkey -v -keystore tictactoe-release-key.jks -keyalg RSA -keysize 2048 -validity 10000 -alias tictactoe-key

